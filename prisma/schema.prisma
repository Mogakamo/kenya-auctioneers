// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
    provider = "prisma-client-js"
}

datasource db {
    provider = "postgres"
    url      = env("DATABASE_URL")
}

model Example {
    id String @id @default(cuid())
}

model Bidder {
    id String @id @default(cuid())
    name String
    email String @unique
    password String
    photo String
    

    @@map("bidders")
}